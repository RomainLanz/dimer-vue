{
  "name": "dimer-vue",
  "version": "2.1.3",
  "description": "Components and helpers to build user documentation websites using Dimer and Vue",
  "main": "dist/dimer-vue.js",
  "module": "dist/dimer-vue.cjs.js",
  "unpkg": "dist/dimer-vue.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "prepublishOnly": "npm run build",
    "commit": "git-cz",
    "pretest": "del-cli test && npm run lint",
    "test": "bili tests/index -c bili.config-test.js && node japaFile.js",
    "posttest": "del-cli test",
    "prebuild": "del-cli dist",
    "build": "bili src/main.js",
    "lint": "standard"
  },
  "keywords": [],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^2.0.3",
    "@dimerapp/markdown": "^3.2.0",
    "@vue/server-test-utils": "^1.0.0-beta.25",
    "@vue/test-utils": "^1.0.0-beta.25",
    "axios-mock-adapter": "^1.16.0",
    "bili": "^4.8.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "dedent": "^0.7.0",
    "del-cli": "^2.0.0",
    "doctoc": "^1.4.0",
    "husky": "^2.4.1",
    "japa": "^2.0.10",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.2",
    "pkg-ok": "^2.3.1",
    "standard": "^12.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "watch": "^1.0.2",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "test"
    ]
  },
  "standard": {
    "ignore": [
      "example",
      "example-output"
    ]
  },
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimerapp/dimer-vue.git"
  },
  "bugs": {
    "url": "https://github.com/dimerapp/dimer-vue/issues"
  },
  "homepage": "https://github.com/dimerapp/dimer-vue#readme",
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  }
}
