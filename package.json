{
  "name": "dimer-vue",
  "version": "1.0.1",
  "description": "Components and helpers to build user documentation websites using Dimer and Vue",
  "main": "dist/dimer-vue.js",
  "module": "dist/dimer-vue.cjs.js",
  "unpkg": "dist/dimer-vue.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "commit": "git-cz",
    "lint": "standard",
    "prepublishOnly": "npm run build",
    "pretest": "del-cli test && npm run lint",
    "test": "bili tests/index -c bili.config-test.js && japa",
    "posttest": "del-cli test",
    "prebuild": "del-cli dist",
    "build": "bili src/main.js",
    "build:example": "bili example/app.js -o example-output --filename output.js --format umd",
    "watch:example": "watch 'npm run build:example' src example"
  },
  "keywords": [],
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.11",
    "@dimerapp/markdown": "^3.1.1",
    "@vue/server-test-utils": "^1.0.0-beta.25",
    "@vue/test-utils": "^1.0.0-beta.25",
    "axios-mock-adapter": "^1.15.0",
    "bili": "^3.1.2",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "dedent": "^0.7.0",
    "del-cli": "^1.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "pkg-ok": "^2.2.0",
    "standard": "^12.0.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "test",
      "japaFile.js"
    ]
  },
  "standard": {
    "ignore": [
      "example",
      "example-output"
    ]
  },
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimerapp/dimer-vue.git"
  },
  "bugs": {
    "url": "https://github.com/dimerapp/dimer-vue/issues"
  },
  "homepage": "https://github.com/dimerapp/dimer-vue#readme"
}
